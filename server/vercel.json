{
    "version": 2,
    "builds": [
        {
            "src": "./dist/server.js",
            "use": "@vercel/node",
            "config": {
                "command": "npm run build",
                "outputDirectory": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./dist/server.js"
        }
    ]
}
